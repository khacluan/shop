<div class="row main-box">
	<div class="col-md-6 col-md-offset-3">
		<%= semantic_form_for current_user, url: '/frontends/update_profile', html: {method: :put} do |f| %>
			<%= f.inputs :profile do %>
				<%= f.input :avatar %>
				<%= f.input :email %>
				<%= f.input :first_name %>
				<%= f.input :last_name%>
				<li class="text-center">
					<button class="btn btn-default btn-primary">Continue</button>
				</li>
			<% end %>
		<% end %>
	</div>
	
</div>
